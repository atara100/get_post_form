<%- include('./header')-%>

    <div class="container">

        <div class="row">
            <div class="col-12 mt-3">
                <h1 class="display-3">Contact Our Company</h1>
                <p>Lorem ipsum dolor sit amet consectetur, adipisicing elit. Beatae, perferendis!</p>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-6">
                <form id="contact-form" action="" method="get" novalidate="novalidate" autocomplete="off">
                    <div class="form-group">
                        <label for="firstName">* First Name:</label>
                        <input type="text" name="firstName" id="firstName" class="form-control">
                    </div>

                    <div class="form-group">
                        <label for="lName">* Last Name:</label>
                        <input type="text" name="lastName" id="lastName" class="form-control">
                    </div>

                    <div class="form-group">
                        <label for="email">* Email:</label>
                        <input type="email" name="email" id="email" class="form-control">
                    </div>

                    <div class="form-group">
                        <label for="phone">* Phone:</label>
                        <input type="tel" name="phone" id="phone" class="form-control">
                    </div>

                    <button type="submit" name="submit" value="Contact Me" class="btn btn-primary">Contact Me>></button>

                </form>
            </div>

        </div>

    </div>


    <script src="https://code.jquery.com/jquery-3.6.0.min.js"
        integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>

    <script type="text/javascript"
        src="https://cdn.jsdelivr.net/npm/jquery-validation@1.19.5/dist/jquery.validate.min.js"></script>
    <script src="./script.js"></script>

    </body>

    </html>